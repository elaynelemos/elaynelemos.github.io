{
  "name": "elaynelemos-gh-page",
  "private": true,
  "description": "Personal website hosted at GitHub to also support a blog with md(markdown)",
  "version": "0.1.0",
  "author": "Elayne Lemos <elayne.lemos@sysvale.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "clean": "gatsby clean",
    "test": "jest",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b main"
  },
  "dependencies": {
    "gatsby": "^2.23.12"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "jest": "26.4.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rahuldkjain/gh-profile-readme-generator"
  },
  "bugs": {
    "url": "https://github.com/rahuldkjain/gh-profile-readme-generator/issues"
  }
}
